{
  "name": "bookmarks-ts",
  "description": "app using ts 2.0 and vue 2.0",
  "author": "dyu",
  "private": true,
  "main": "nw/main.js",
  "inject_js_start": "nw/start.js",
  "webkit": {
    "plugin": false
  },
  "window": {
    "icon": "nw/icon.png"
  },
  "scripts": {
    "dev": "webpack-dev-server --inline --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "bk-dev": "SRC_DIR=./bookmarklet DEST_FILE=bookmarklet.js webpack-dev-server --inline --hot",
    "bk-build": "SRC_DIR=./bookmarklet DEST_FILE=bookmarklet.js cross-env NODE_ENV=production webpack --progress --hide-modules",
    "bk-nw-build": "SRC_DIR=./bookmarklet-nw DEST_FILE=bookmarklet-nw.js cross-env NODE_ENV=production webpack --progress --hide-modules",
    "build-all": "yarn run build && yarn run bk-build && yarn run bk-nw-build"
  },
  "dependencies": {
    "pavilion": "^2.0.3",
    "toprogress": "^0.1.3",
    "vue": "2.1.10",
    "vue-router": "2.1.3",
    "vueds": "^0.8.2",
    "vueds-ui": "^0.7.1",
    "vuets": "^0.10.0",
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "cross-env": "^3.2.4",
    "css-loader": "^0.27.3",
    "cssnano": "^3.7.5",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "json-loader": "^0.5.4",
    "less": "^2.7.2",
    "less-loader": "^2.2.3",
    "node-sass": "^4.5.0",
    "node-static": "^0.7.9",
    "noop2": "^2.0.0",
    "postcss-loader": "^1.3.3",
    "postcss-unique-selectors": "^2.0.2",
    "sass-loader": "^6.0.3",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "ts-loader": "^2.0.1",
    "typescript": "2.2.2",
    "url-loader": "^0.5.8",
    "vue-loader": "9.9.5",
    "vue-template-compiler": "2.1.10",
    "webpack": "2.2.1",
    "webpack-closure-compiler": "2.0.2",
    "webpack-dev-server": "2.2.1"
  }
}
